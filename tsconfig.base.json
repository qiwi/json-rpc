{
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "es5",
    "lib": [
      "esnext"
    ],
    "noImplicitAny": true,
    "strict": true,
    "sourceMap": true,
    "declaration": true,
    "noEmitHelpers": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "importHelpers": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "composite": true,
    "strictNullChecks": true,
    "experimentalDecorators": true
  }
}
